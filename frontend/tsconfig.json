{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es3",
      "jsx": "react",
      "jsxFactory": "h",
      "jsxFragmentFactory": "Fragment",
      "strict": true,
      "strictPropertyInitialization": false,
      "allowSyntheticDefaultImports": true,
      "allowJs": false,
      "moduleResolution": "node",
      "lib": ["dom"]
    }
  }  